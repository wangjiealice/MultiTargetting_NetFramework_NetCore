  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net461' ">
    <ProjectReference Include="..\Project_Standard\Project_Standard.csproj" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Project_Standard_1_6\Project_Standard_1_6.csproj" />
  </ItemGroup>
  
</Project>